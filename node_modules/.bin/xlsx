#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_xlsx-style@0.8.13@xlsx-style/bin/xlsx.njs" "$@"
  ret=$?
else 
  node  "$basedir/../_xlsx-style@0.8.13@xlsx-style/bin/xlsx.njs" "$@"
  ret=$?
fi
exit $ret
